mixin block_gallery(object)
    .block_gallery&attributes(attributes)
        .container
            .block_gallery-list.js-block-gallery-list
                .block_gallery-col.block_gallery-full
                    a(href="/images/pages/news/gallery1-original.jpg" data-fancybox="block_gallery").block_gallery-item
                        picture
                            source(
                                media="(max-width: 576.98px)"
                                srcset="/images/pages/news/gallery1-mobile.jpg"
                            )
                            source(
                                media="(max-width: 1007.98px)"
                                srcset="/images/pages/news/gallery1-tablet.jpg"
                            )
                            img(
                                src="/images/pages/news/gallery1.jpg"
                            )
                .block_gallery-col.block_gallery-half
                    a(href="/images/pages/news/gallery2-original.jpg" data-fancybox="block_gallery").block_gallery-item
                        picture
                            source(
                                media="(max-width: 576.98px)"
                                srcset="/images/pages/news/gallery2-mobile.jpg"
                            )
                            source(
                                media="(max-width: 1007.98px)"
                                srcset="/images/pages/news/gallery2-tablet.jpg"
                            )
                            img(
                                src="/images/pages/news/gallery2.jpg"
                            )
                .block_gallery-col.block_gallery-half
                    a(href="/images/pages/news/gallery3-original.jpg" data-fancybox="block_gallery").block_gallery-item
                        picture
                            source(
                                media="(max-width: 576.98px)"
                                srcset="/images/pages/news/gallery3-mobile.jpg"
                            )
                            source(
                                media="(max-width: 1007.98px)"
                                srcset="/images/pages/news/gallery3-tablet.jpg"
                            )
                            img(
                                src="/images/pages/news/gallery3.jpg"
                            )

                .block_gallery-col.block_gallery-one-third
                    a(href="/images/pages/news/gallery4-original.jpg" data-fancybox="block_gallery").block_gallery-item
                        picture
                            source(
                                media="(max-width: 576.98px)"
                                srcset="/images/pages/news/gallery4-mobile.jpg"
                            )
                            source(
                                media="(max-width: 1007.98px)"
