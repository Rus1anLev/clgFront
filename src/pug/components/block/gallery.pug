                                media="(max-width: 576.98px)"
                                srcset="/images/pages/news/gallery5-mobile.jpg"
                            )
                            source(
                                media="(max-width: 1007.98px)"
                                srcset="/images/pages/news/gallery5-tablet.jpg"
                            )
                            img(
                                src="/images/pages/news/gallery5.jpg"
                            )
                .block_gallery-col.block_gallery-one-third
                    a(href="/images/pages/news/gallery6-original.jpg" data-fancybox="block_gallery").block_gallery-item
                        picture
                            source(
                                media="(max-width: 576.98px)"
                                srcset="/images/pages/news/gallery6-mobile.jpg"
                            )
                            source(
                                media="(max-width: 1007.98px)"
                                srcset="/images/pages/news/gallery6-tablet.jpg"
                            )
                            img(
                                src="/images/pages/news/gallery6.jpg"
                            )
            .progress-wrapper.block_gallery-progress
                .progress(role="progressbar" aria-valuemin="0" aria-valuemax="100")